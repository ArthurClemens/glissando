{
  "name": "test-glissando",
  "private": true,
  "description": "Test Glissando",
  "version": "0.8.5",
  "license": "MIT",
  "scripts": {
    "npm-check-updates": "../../node_modules/npm-check-updates/bin/cli.js -u",
    "test": "ava --timeout=2m",
    "test:watch": "ava --watch",
    "prettier": "prettier --config ../../.prettierrc.json --write \"./test/*.ts\""
  },
  "dependencies": {
    "@ava/typescript": "^3.0.1",
    "ava": "^5.2.0",
    "browser-env": "^3.3.0",
    "glissando": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
